      },
      "source": [
        "target = train.Sentiment.values\n",
        "y_target = to_categorical(target)\n",
        "num_classes = y_target.shape[1]\n",
        "print(num_classes)\n",
        "print(train.Sentiment.values)"
      ],
      "execution_count": 0,
      "outputs": []
    },
